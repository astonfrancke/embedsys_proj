#include "sensors_test.h"

#include "sensors.h"
#include "display.h"


void sensors_test_program(void) {
	lcd_init();
	test_get_humi_temp();
}


void test_get_humi_temp(void) {
	while(1) {
		lcd_set_pos(LINE1);
		lcd_write_string(get_humi_temp());
		HAL_Delay(500);
	}
}
